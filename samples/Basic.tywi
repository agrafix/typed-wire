module Basic;

import Other;

type Foo {
   someField : Int;
   someOtherField : Maybe<Bool>;
   barBool : Bar<Bool>;
   person : Other.Person;
}

type Bar<t> {
   someField : t;
   moreFields : Int;
}

enum Psy {
   Simple;
   Tagged(Foo);
}

enum FooOrBar<t> {
    Foo(Foo);
    BarInt(Bar<Int>);
    BarGeneral(Bar<t>);
}

api fooApi {
    ["X-ApiKey" = String];
    getFoo = get("foo" / Int)["X-Super-Header" = String] : Foo;
    setFoo = post("foo" / String) : Foo -> Bool;
}